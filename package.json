{
  "private": true,
  "type": "module",
  "name": "template-storybook-vue",
  "version": "0.4.0",
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "^18.20.4 || ^20.17.0 || >=22.7.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,vue}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.1.1",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/vue3": "^8.6.11",
    "@storybook/vue3-vite": "^9.0.16",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.27.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.3",
    "storybook": "^8.6.14",
    "vite": "^5.4.17"
  },
  "dependencies": {
    "pinia": "^2.3.1",
    "vue": "^3.5.15"
  },
  "overrides": {
    "@typescript-eslint/utils": {
      "eslint": "$eslint"
    }
  }
}
