{
  "private": true,
  "type": "module",
  "name": "template-storybook-vue",
  "version": "0.4.0",
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "^18.20.4 || ^20.17.0 || >=22.7.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,vue}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.3.0",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/vue3": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.1.11",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "storybook": "^10.1.11",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.26"
  },
  "overrides": {
    "@typescript-eslint/utils": {
      "eslint": "$eslint"
    }
  }
}
