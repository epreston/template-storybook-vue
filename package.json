{
  "private": true,
  "type": "module",
  "name": "template-storybook-vue",
  "version": "0.4.0",
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "^18.20.4 || ^20.17.0 || >=22.7.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,vue}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.1.1",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/vue3": "^8.4.6",
    "@storybook/vue3-vite": "^8.5.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.18.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "storybook": "^8.5.0",
    "vite": "^5.4.14"
  },
  "dependencies": {
    "pinia": "^2.3.1",
    "vue": "^3.5.13"
  },
  "overrides": {
    "@typescript-eslint/utils": {
      "eslint": "$eslint"
    }
  }
}
